{
    "kind": "singleType",
    "collectionName": "homes",
    "info": {
        "singularName": "home",
        "pluralName": "homes",
        "displayName": "Home"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {
        "i18n": {
            "localized": true
        }
    },
    "attributes": {
        "banners": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::banner.banner"
        },
        "homeTitle": {
            "type": "string",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "homeDesc": {
            "type": "text",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "exploreButton": {
            "type": "string",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "SEO": {
            "type": "component",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "component": "shared.seo",
            "repeatable": false
        },
        "homeImg": {
            "type": "media",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "homeNewsTitle": {
            "type": "string",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "homeNewsDesc": {
            "type": "text",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "homeDetails": {
            "type": "component",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "component": "shared.page-elements",
            "repeatable": true
        },
        "articles": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::article.article"
        }
    }
}
